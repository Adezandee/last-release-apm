  },
  "devDependencies": {
    "chai": "~4.1.0",
    "coveralls": "~2.13.1",
    "cz-conventional-changelog": "~2.0.0",
    "eslint": "~4.2.0",
    "eslint-config-airbnb-base": "~11.2.0",
    "eslint-plugin-import": "~2.7.0",
    "mocha": "~3.4.2",
    "nock": "~9.0.13",
    "nyc": "11.0.3",
    "semantic-release": "~6.3.6"
